<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ionicframework.myfirstapp102059" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
